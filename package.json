{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Ejemplos en React",
  "main": "/public/app.js",
  "scripts": {
    "build": "webpack --colors --progress",
    "start": "webpack-dev-server --colors --progress --mode development --open",
    "buildprod": "npm run build && npm run server",
    "prod": "webpack -p",
    "test": "jest",
    "server": "node jsonserver"
  },
  "jest": {
    "moduleFileExtensions": [
      "",
      "js",
      "json"
    ],
    "modulePaths": [
      "./src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "/.*\\-snapshot\\.js$",
    "moduleNameMapper": {
      "^testUtils": "./src/utils/tests.js",
      "^.+\\.(s?css|less)$": "./src/utils/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "./src/utils/fileMock.js"
    },
    "setupFiles": [
      "./src/utils/setupJest.js"
    ]
  },
  "author": "Victor Tataje <percytataje@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.12",
    "@types/jest": "^22.2.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "jest-junit": "^4.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.2.0",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-jest": "^23.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/recompose": "^0.26.1",
    "axios": "^0.18.0",
    "jest-styled-components": "^5.0.1",
    "json-server": "^0.14.0",
    "raf": "3.3.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-refetch": "^1.0.4",
    "recompose": "^0.27.1",
    "robust-point-in-polygon": "^1.0.3",
    "source-map-loader": "^0.2.3",
    "styled-components": "^3.3.2"
  }
}
